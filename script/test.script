import "std/io.script"
import "std/types.script"

void PrintFibonacciSequence(int amount)
{
    int x;
    int y = 1;
    int z;

    for (int i = 0; i < amount; i = i + 1)
    {
        WriteLine(ToString(x));

        z = x + y;
        x = y;
        y = z;
    }
}

void main()
{
    WriteLine("How many Fibonacci numbers should be calculated?");
    int amount = ToInt(GetLine());
    PrintFibonacciSequence(amount);
}